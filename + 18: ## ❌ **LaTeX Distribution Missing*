+    381:         test_messages = self.create_test_messages()
+    382:         security_messages = self.create_security_messages()
+    383:         
+    384:         all_messages = hello_messages + test_messages + security_messages
+    385:         
+    386:         print(f"   ‚úì Created {len(hello_messages)} Hello World messages")
+    387:         print(f"   ‚úì Created {len(test_messages)} test messages")
+    388:         print(f"   ‚úì Created {len(security_messages)} security messages")
+    389:         print(f"   ‚úì Total messages: {len(all_messages)}")
+    390:         print()
